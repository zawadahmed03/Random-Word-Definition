{
  "manifest_version": 2,
  "name": "Random Word Definition",
  "description": "Learn a new word everytime you open your browser",
  "version": "1.0.0",
  "icons": { "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://random-word-api.herokuapp.com/",
    "https://api.dictionaryapi.dev/"
  ]
}
